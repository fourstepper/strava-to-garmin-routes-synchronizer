<p style="color: green"><%= notice %></p>
<% content_for :title, "Routes" %>
<h1>Routes</h1>

<%= button_to "Run Strava code", "custom", method: :get %>

<div id="routes">
  <% @routes.each do |route| %>
    <%= render route %>
    <p>
      <%= link_to "Show this route", route %>
    </p>
  <% end %>
</div>

<%= link_to "New route", new_route_path %>
